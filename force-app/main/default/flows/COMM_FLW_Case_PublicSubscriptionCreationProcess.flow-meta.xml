    <decisions>
        <description>Check if Subscription is conditions.</description>
        <name>COMM_IsSubscriptionInConditions</name>
        <label>Is Subscription In Conditions?</label>
        <locationX>1436</locationX>
        <locationY>384</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes_COMM_IsSubscriptionInConditions</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>COMM_XDaysFromToday</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Record.COMM_EndDate__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.COMM_Active__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RecordType.DeveloperName</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>COMM_CancelPublicSubscription</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.COMM_Subscription__r.COMM_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Process Concluded</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>COMM_SetEmailRecipients</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
