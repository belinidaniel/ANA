                </rightValue>
            </conditions>
            <connector>
                <targetReference>COMM_Assignment_StoreEmailInPortugueseList</targetReference>
            </connector>
            <label>Portuguese (Portugal)</label>
        </rules>
        <rules>
            <name>COMM_Outcome_Spanish</name>
