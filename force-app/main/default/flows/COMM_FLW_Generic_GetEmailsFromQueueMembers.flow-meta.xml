        <rules>
            <name>COMM_Outcome_Portuguese</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Collect_User_Emails.LanguageLocaleKey</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
