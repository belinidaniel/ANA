<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>COMM_VerifyExpiredDate</fullName>
    <active>true</active>
    <description>Check if Capacity date is expired</description>
    <errorConditionFormula>AND(
NOT(ISNEW()),
PRIORVALUE(COMM_ValidTo__c) &lt; NOW())</errorConditionFormula>
    <errorMessage>Expired Capacities cannot be changed</errorMessage>
</ValidationRule>
