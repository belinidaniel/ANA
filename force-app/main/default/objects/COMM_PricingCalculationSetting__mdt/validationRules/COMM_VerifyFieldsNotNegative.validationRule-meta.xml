<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>COMM_VerifyFieldsNotNegative</fullName>
    <active>true</active>
    <description>Verify that the Required Fields are not Negative</description>
    <errorConditionFormula>COMM_MinimumChargedDays__c &lt; 0 || COMM_MinimumRequiredHours__c &lt; 0</errorConditionFormula>
    <errorMessage>The fields can not be negative</errorMessage>
</ValidationRule>
