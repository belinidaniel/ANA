<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>COMM_AlarmisticPercExceeded__c</fullName>
    <description>Formula to identify if the Alarmistic % on related Product was exceeded.</description>
    <formula>AND(
 NOT(ISBLANK(COMM_OccupancyRate__c)),
 NOT(ISBLANK(COMM_Product__r.COMM_Alarmistic__c)),
 COMM_OccupancyRate__c &gt;= COMM_Product__r.COMM_Alarmistic__c
)</formula>
    <label>Alarmistic % Exceeded</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
