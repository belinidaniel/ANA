<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>COMM_CardTaxCode__c</fullName>
    <description>Code used in SAP to identify the tax applied on the invoiced Card item</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(COMM_Account__r.BillingCountryCode, &apos;PT&apos;) || ISPICKVAL(COMM_Account__r.BillingCountryCode, &apos;&apos;),
CASE(COMM_SubscriptonProduct__r.COMM_Tax__c, &apos;Continent&apos;, &apos;PP&apos;, &apos;Madeira&apos;, &apos;PP&apos;,&apos;Azores&apos;,&apos;PP&apos;, &apos;PP&apos;),
CASE(COMM_SubscriptonProduct__r.COMM_Tax__c, &apos;Continent&apos;, &apos;PP&apos;, &apos;Madeira&apos;, &apos;PP&apos;,&apos;Azores&apos;,&apos;PP&apos;, &apos;PP&apos;))</formula>
    <label>Card Tax Code</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
