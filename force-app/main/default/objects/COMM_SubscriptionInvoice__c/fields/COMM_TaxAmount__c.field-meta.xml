<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>COMM_TaxAmount__c</fullName>
    <description>Amount charged as Tax for the Subscription</description>
    <formula>COMM_SubscriptionAmount__c * CASE(COMM_SubscriptonProduct__r.COMM_Tax__c, &apos;Continent&apos;, $CustomMetadata.COMM_TaxValueConfiguration__mdt.Continent.COMM_TaxValue__c, &apos;Azores&apos;, $CustomMetadata.COMM_TaxValueConfiguration__mdt.Azores.COMM_TaxValue__c, &apos;Madeira&apos;, $CustomMetadata.COMM_TaxValueConfiguration__mdt.Madeira.COMM_TaxValue__c , $CustomMetadata.COMM_TaxValueConfiguration__mdt.Continent.COMM_TaxValue__c) / (1 + CASE(COMM_SubscriptonProduct__r.COMM_Tax__c, &apos;Continent&apos;, $CustomMetadata.COMM_TaxValueConfiguration__mdt.Continent.COMM_TaxValue__c, &apos;Azores&apos;, $CustomMetadata.COMM_TaxValueConfiguration__mdt.Azores.COMM_TaxValue__c, &apos;Madeira&apos;, $CustomMetadata.COMM_TaxValueConfiguration__mdt.Madeira.COMM_TaxValue__c , $CustomMetadata.COMM_TaxValueConfiguration__mdt.Continent.COMM_TaxValue__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Subscription Tax Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>5</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
